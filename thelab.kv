#:import layout_examples layout_examples
#:import widget_examples widget_examples
#:import navigation_screen_manager navigation_screen_manager
#:import boxlayout_with_action_bar boxlayout_with_action_bar

MyScreenManager:

<MyScreenManager>:
  Screen:
    name: "screen1"
    BoxLayout:
      Button:
        text: "go to screen images"
        on_press: app.manager.push("ImagesExample")
      Button:
        text: "go to screen 3"
        on_press: app.manager.push("screen3")
  Screen:
    name: "ImagesExample"
    BoxLayoutWithActionBar:
      # title: "Images"
      ImagesExample:
  Screen:
    name: "screen2"
    Button:
      text: "Button2"
      on_press: app.manager.pop()
  Screen:
    name: "screen3"
    Button:
      text: "Button3"
      on_press: app.manager.pop()
