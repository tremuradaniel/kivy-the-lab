#:import layout_examples layout_examples
#:import widget_examples widget_examples
#:import navigation_screen_manager navigation_screen_manager

MyScreenManager:

<MyScreenManager@NavigationScreenManager>:
  Screen:
    name: "screen1"
    BoxLayout:
      Button:
        text: "go to screen 2"
        on_press: root.push("screen2")
      Button:
        text: "go to screen 3"
        on_press: root.push("screen3")
  Screen:
    name: "screen2"
    Button:
      text: "Button2"
      on_press: root.pop()
  Screen:
    name: "screen3"
    Button:
      text: "Button3"
      on_press: root.pop()
